{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "build vxrn",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}/packages/vxrn",
      "program": "${workspaceFolder}/node_modules/.bin/tamagui-build",
      "args": []
    },
    {
      "type": "node",
      "request": "launch",
      "name": "vxrn dev in examples/vxs",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}/examples/vxs",
      "env": {
        "DEBUG": "true"
      },
      "program": "${workspaceFolder}/packages/vxrn/src/cli.ts",
      // "program": "${workspaceFolder}/packages/vxrn/dist/esm/cli.mjs",
      "args": [
        "dev",
        "--clean"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "outputCapture": "std",
      "name": "vxrn dev in examples/expo-blank",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}/examples/expo-blank",
      "env": {
        "DEBUG": "true"
      },
      "program": "${workspaceFolder}/packages/vxrn/src/cli.ts",
      "args": [
        "dev",
        "--clean"
      ]
    }
  ]
}
